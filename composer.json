    "type": "library",
    "require": {
        "endroid/qr-code": "^5.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nusantara\\QRCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        "Nusantara\\QRCode\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "authors": [
        {
            "name": "Fadli Azka Prayogi",
            "email": "fadliazkaprayogi1@gmail.com"
        }
    ],
    "minimum-stability": "stable"
